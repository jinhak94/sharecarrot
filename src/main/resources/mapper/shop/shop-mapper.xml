<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="shop">

	<insert id="shopEnroll">
		insert into 
			shop
		values(
			#{shopId},
			default,
			default,
			null,
			#{memberId}
		)
	</insert>

</mapper>